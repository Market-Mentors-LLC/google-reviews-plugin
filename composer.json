{
  "name": "market-mentors/google-reviews-plugin",
  "description": "Uses the google places API to display the latest review.",
  "type": "wordpress-plugin",
  "autoload": {
    "psr-4": {
      "MarketMentors\\GoogleReviewsPlugin\\": ""
    }
  },
  "authors": [
    {
      "name": "Market Mentors, LLC.",
      "email": "accounts@marketmentors.com",
      "role": "Company"
    },
    {
      "name": "Tyler Seabury",
      "email": "tyler@marketmentors.com",
      "role": "Developer"
    },
    {
      "name": "Devon LaChance",
      "email": "dlachance@marketmentors.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "yahnis-elsts/plugin-update-checker": "^5.0",
    "guzzlehttp/guzzle": "^7.10",
    "kevinrob/guzzle-cache-middleware": "^7.0",
    "symfony/cache": "^7.3",
    "league/flysystem": "^3.30"
  },
  "require-dev": {
    "php-stubs/wordpress-stubs": "^6.8",
    "php-parallel-lint/php-parallel-lint": "^1.4"
  },
  "scripts": {
    "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude ./vendor --no-autoloader ."
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}